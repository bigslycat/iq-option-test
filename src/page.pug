doctype html
html( lang = 'ru' )
  head
    meta( charset = 'utf-8' )
    meta(
      name = 'viewport'
      content = (
        Object.entries({
          width: 'device-width',
          'initial-scale': 1,
          'minimum-scale': 1,
          'maximum-scale': 1,
          'user-scalable': 'no',
        })
          .map(entry => entry.join('='))
          .join(',')
      )
    )
    title iq-option-test
  body
    #app
